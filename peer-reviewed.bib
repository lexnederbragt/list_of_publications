	month = oct,
	year = {2012},
	keywords = {High-throughput sequencing, Illumina, 454, metagenomics, Metabolic-profiling, Microbial community, Taxonomic-profiling},
	pages = {106--113},
	file = {ScienceDirect Full Text PDF:/Users/alexajo/Zotero/storage/3URHMFMK/Logares et al. - 2012 - Environmental microbiology through the lens of hig.pdf:application/pdf;ScienceDirect Snapshot:/Users/alexajo/Zotero/storage/LLH69ITG/S0167701212002527.html:text/html}
}

