	month = sep,
	year = {2011},
	keywords = {Evolution, Genetics and genomics, Immunology, Molecular biology},
	pages = {207--210},
	file = {Full Text PDF:/Users/alexajo/Zotero/storage/W7QRC5VN/Star et al. - 2011 - The genome sequence of Atlantic cod reveals a uniq.pdf:application/pdf;Snapshot:/Users/alexajo/Zotero/storage/X8P3GNCZ/nature10342.html:text/html}
