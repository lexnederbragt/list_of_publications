	pages = {31235},
	file = {Full Text PDF:/Users/alexajo/Zotero/storage/ICXELUUG/Star et al. - 2016 - Genomic characterization of the Atlantic cod sex-l.pdf:application/pdf;Snapshot:/Users/alexajo/Zotero/storage/JESU3974/srep31235.html:text/html}
}

