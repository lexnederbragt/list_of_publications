@article{lie_loss_2018,
    title = {{Loss of stomach, loss of appetite? Sequencing of the ballan wrasse (Labrus bergylta) genome and intestinal transcriptomic profiling illuminate the evolution of loss of stomach function in fish}},
    author = {Lie, Kai K. and Tørresen, Ole K. and Solbakken, Monica Hongrø and Rønnestad, Ivar and Tooming-Klunderud, Ave and Nederbragt, Alexander J. and Jentoft, Sissel and Sæle, Øystein},
    year = {2018},
    journal = {{BMC Genomics}},
