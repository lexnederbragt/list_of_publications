@article{simovski_gsuite_2017,
    title = {{GSuite HyperBrowser: integrative analysis of dataset collections across the genome and epigenome}},
    author = {Simovski, Boris and Vodák, Daniel and Gundersen, Sveinung and Domanska, Diana and Azab, Abdulrahman and Holden, Lars and Holden, Marit and Grytten, Ivar and Rand, Knut and Drabløs, Finn and Johansen, Morten and Mora, Antonio and Lund-Andersen, Christin and Fromm, Bastian and Eskeland, Ragnhild and Gabrielsen, Odd Stokke and Ferkingstad, Egil and Nakken, Sigve and Bengtsen, Mads and Nederbragt, Alexander Johan and Sif Thorarensen, Hildur and Andreas Akse, Johannes and Glad, Ingrid and Hovig, Eivind and Sandve, Geir Kjetil},
